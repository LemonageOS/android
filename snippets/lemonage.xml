<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote
    name="lemonage"
    fetch="https://github.com/LemonageOS"
    revision="lineage-18.1" />

  <project path="android" name="android_manifest" remote="lemonage" />
  <project path="art" name="android_art" remote="lemonage" />
  <project path="build/make" name="android_build" remote="lemonage" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="external/roboto-fonts" name="android_external_roboto-fonts" remote="lemonage" revision="android11-platform-release" />
  <project path="frameworks/av" name="android_frameworks_av" remote="lemonage" />
  <project path="frameworks/libs/systemui" name="android_frameworks_libs_systemui" remote="lemonage" revision="android11-platform-release" />
  <project path="frameworks/opt/telephony" name="android_frameworks_opt_telephony" remote="lemonage" />
  <project path="lineage-sdk" name="android_lineage-sdk" remote="lemonage" />
  <project path="packages/apps/404Launcher" name="android_packages_apps_404Launcher" remote="lemonage" revision="rippa" />
  <project path="packages/apps/DeskClock" name="android_packages_apps_DeskClock" remote="lemonage" />
  <project path="packages/apps/Recorder" name="android_packages_apps_Recorder" remote="lemonage" revision="lineage-20.0" />
  <project path="packages/apps/SettingsIntelligence" name="android_packages_apps_SettingsIntelligence" remote="lemonage" revision="lineage-20.0" />
  <project path="packages/apps/SetupWizard" name="android_packages_apps_SetupWizard" remote="lemonage" />
  <project path="packages/apps/ThemePicker" name="android_packages_apps_ThemePicker" remote="lemonage" />
  <project path="packages/apps/WallpaperPicker2" name="android_packages_apps_WallpaperPicker2" remote="lemonage" />
  <project path="packages/apps/WebView" name="android_packages_apps_WebView" remote="lemonage" revision="main" />
  <project path="packages/services/Car" name="android_packages_services_Car" remote="lemonage" />
  <project path="system/core" name="android_system_core" remote="lemonage" />
  <project path="vendor/certification" name="android_vendor_certification" remote="lemonage" revision="main" />
  <project path="vendor/support" name="android_vendor_support" remote="lemonage" />

</manifest>
